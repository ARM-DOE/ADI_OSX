$(OBJ_DIR)/dbconn.o: dbconn.c dbconn_pgsql.h $(COMP_HOME)/include/dbconn.h $(COMP_HOME)/include/messenger.h dbconn_private.h dbconn_sqlite.h sqlite3.h dbconn_wspc.h
$(OBJ_DIR)/dbconn_pgsql.o: dbconn_pgsql.c dbconn_pgsql.h $(COMP_HOME)/include/dbconn.h $(COMP_HOME)/include/messenger.h
$(OBJ_DIR)/dbconn_sqlite.o: dbconn_sqlite.c dbconn_sqlite.h $(COMP_HOME)/include/dbconn.h $(COMP_HOME)/include/messenger.h sqlite3.h
