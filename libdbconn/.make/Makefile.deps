$(OUT_DIR)/libdbconn.a: $(OBJ_DIR)/dbconn.o $(OBJ_DIR)/dbconn_pgsql.o $(OBJ_DIR)/dbconn_sqlite.o $(OBJ_DIR)/dbconn_version.o $(OBJ_DIR)/dbconn_wspc.o $(OBJ_DIR)/sqlite3.o

