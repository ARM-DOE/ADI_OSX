TARGET_TYPE = SO
TARGET      = libcds3.so

include $(MKINC_DIR)/make.defs

IFLAGS += \
-I$(AFL_LIB)/include \
-I$(UDUNITS2_LIB)/include \
-I$(GLIB_LIB)/include \
-I$(GLIB_LIB)/include/glib-2.0 \
-I$(GLIB_LIB)/lib/glib-2.0/include

LFLAGS += \
-L$(AFL_LIB)/lib$(LIB64) \
-L$(UDUNITS2_LIB)/lib$(LIB64)

RFLAGS += \
-R$(AFL_LIB)/lib$(LIB64) \
-R$(UDUNITS2_LIB)/lib$(LIB64)

LIBS   += -lmsngr -ludunits2 -lexpat -lm 

include $(MKINC_DIR)/make.rules

